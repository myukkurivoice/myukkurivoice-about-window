{
  "name": "myukkurivoice-about-window",
  "version": "1.14.3",
  "description": "'About App' window for Electron application",
  "main": "src/index.js",
  "scripts": {
    "dep": "npm install",
    "build": "tsc -p src/",
    "watch": "tsc --watch -p src/",
    "tslint": "tslint -p ./src",
    "stylelint": "stylelint styles/*.css",
    "lint": "npm run tslint && npm run stylelint",
    "example": "NODE_ENV=production electron ./example",
    "debug": "electron ./example",
    "start": "npm run dep && npm run build && npm run example",
    "preversion": "npm run lint && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myukkurivoice/myukkurivoice-about-window.git"
  },
  "keywords": [
    "Electron",
    "electron-component",
    "about",
    "window"
  ],
  "author": "taku-o <mail@nanasi.jp> (https://nanasi.jp/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/myukkurivoice/myukkurivoice-about-window/issues"
  },
  "homepage": "https://github.com/myukkurivoice/myukkurivoice-about-window#readme",
  "devDependencies": {
    "@types/node": "^12.6.6",
    "electron": "^5.0.7",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "semver": "^5.5.0"
  }
}
